{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "plugins": ["async-await"],
    "rules": {
        "async-await/space-after-async": 2,
        "async-await/space-after-await": 2,
        "no-undef": 2,
        "no-unused-vars": 1
    }
}